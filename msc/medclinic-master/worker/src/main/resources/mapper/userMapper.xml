<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.training.mapper.UserMapper">
    <resultMap id="userResultMap" type="com.training.User">
        <id property="u_id" column="id"/>
        <result column="user_id" property="user.id"/>
        <result column="user_login" property="user.login"/>
        <result column="user_email" property="user.email"/>
        <result column="user_password" property="user.password"/>
    </resultMap>
</mapper>